{
  "name": "mailcore2-ios",
  "version": "0.6.4",
  "summary": "Mailcore 2 for iOS",
  "description": "MailCore 2 provide a simple and asynchronous API to work with e-mail protocols IMAP, POP and SMTP.",
  "homepage": "http://libmailcore.com",
  "license": {
    "type": "BSD",
    "file": "LICENSE"
  },
  "authors": "MailCore Authors",
  "source": {
    "git": "http://github.com/agent8/mailcore2.git",
    "branch": "release"
  },
  "platforms": {
    "ios": 10.0
  },
  "header_dir": "MailCore",
  "requires_arc": false,
  "public_header_files": "cocoapods-build/include/MailCore/*.h",
  "preserve_paths": "cocoapods-build/include/MailCore/*.h",
  "vendored_libraries": "cocoapods-build/lib/libEdoMailcore2.a",
  "libraries": ["xml2", "iconv", "z", "c++", "resolv"],
  "prepare_command": "./scripts/edo-build-mailcore2-ios-cocoapod.sh"
